{
  "name": "rsvp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "node": "NODE_PATH=$NODE_PATH:./lib node -r dotenv/config -r babel-core/register",
    "dev": "concurrently 'npm run node .' 'mongod'",
    "start": "npm run node",
    "lint": "standard",
    "lint-fix": "standard --fix",
    "mocha": "NODE_ENV=test mocha -r dotenv/config -r babel-core/register -r should",
    "test": "npm run lint && npm run mocha test/*/*"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "engines": {
    "node": ">=6.7.0",
    "npm": "3.8.x"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.13.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.0",
    "babel-preset-stage-3": "^6.11.0",
    "babelify": "^7.3.0",
    "concurrently": "^2.1.0",
    "dotenv": "^2.0.0",
    "envify": "^3.4.0",
    "graphql": "^0.7.2",
    "hotglue": "0.0.7",
    "joiql-mongo": "^1.0.10",
    "json2csv": "^3.7.1",
    "koa": "^2.0.0-alpha.4",
    "koa-basic-auth": "^2.0.0",
    "lokka": "^1.7.0",
    "lokka-transport-http": "^1.4.0",
    "moment": "^2.15.2",
    "qs": "^6.3.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "unikoa": "0.0.1",
    "unikoa-bootstrap": "0.0.2",
    "unikoa-react-render": "0.0.4",
    "universal-tree": "0.0.2",
    "veact": "0.0.5"
  },
  "devDependencies": {
    "babel-plugin-rewire": "^1.0.0",
    "cheerio": "^0.22.0",
    "mocha": "^3.0.2",
    "should": "^11.1.0",
    "sinon": "^1.17.6",
    "standard": "^8.1.0"
  }
}
